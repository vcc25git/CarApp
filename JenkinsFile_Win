pipeline {
	agent any
	
	stages {
		stage('Checkout'){
			steps {
				git 'https://github.com/vcc25git/CarApp.git'
			}
		}
		stage('Build'){
			steps {
				bat 'mvn clean compile'
			}
		}
	

		stage('Testing'){
			steps {
			 	bat 'test'
			 	junit '**/target/surefire-reports/TEST-*.xml'
			 }
		}

		stage('Package') {
			steps {
				bat 'mvn package'
			}
		}		
	}
}